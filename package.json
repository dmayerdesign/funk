{
  "name": "funk",
  "version": "0.0.0",
  "scripts": {
    "build": "npm run ui::build && npm run functions::build",
    "deploy": "firebase deploy",
    "start": "npm run ui::start",
    "test": "ng test --project=web --passWithNoTests",
    "ui::build": "ng build --prod --configuration=development; ng run web:server --configuration=development",
    "ui::build::prod": "ng build --prod --configuration=production; ng run web:server --configuration=production",
    "ui::deploy": "firebase deploy --only hosting",
    "ui::develop": "ng serve --project=web",
    "ui::start": "npx angular-http-server --path ui/web/dist/web-browser -p 4200",
    "ui::lint": "ng lint --project=web",
    "functions::build": "node functions-prebuild && tsc --project functions/tsconfig.json && node functions-postbuild",
    "functions::build::prod": "node functions-prebuild -c production && tsc --project functions/tsconfig.json && node functions-postbuild",
    "functions::build::watch": "node functions-prebuild && tsc-watch --project functions/tsconfig.json --onSuccess \"node functions-postbuild\"",
    "functions::deploy": "firebase deploy --only functions",
    "functions::serve": "firebase serve --only functions,firestore --port 5200",
    "functions::shell": "npm run functions::build && firebase functions:shell",
    "functions::logs": "firebase functions:log",
    "ng": "ng"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "8.1.3",
    "@angular/cdk": "8.1.2",
    "@angular/common": "~8.1.2",
    "@angular/compiler": "~8.1.2",
    "@angular/core": "~8.1.2",
    "@angular/fire": "5.2.1",
    "@angular/flex-layout": "8.0.0-beta.26",
    "@angular/forms": "~8.1.2",
    "@angular/material": "8.1.2",
    "@angular/platform-browser": "~8.1.2",
    "@angular/platform-browser-dynamic": "~8.1.2",
    "@angular/platform-server": "~8.1.2",
    "@angular/pwa": "0.801.3",
    "@angular/router": "~8.1.2",
    "@angular/service-worker": "~8.1.2",
    "@dannymayer/vex": "0.3.3",
    "cookie-parser": "1.4.4",
    "core-js": "^2.5.4",
    "express": "4.17.1",
    "firebase": "6.3.4",
    "firebase-admin": "8.3.0",
    "firebase-functions": "3.2.0",
    "gsap": "2.1.3",
    "hammerjs": "^2.0.8",
    "lodash": "4.17.15",
    "ng-click-outside": "4.0.0",
    "rxjs": "~6.5.2",
    "stripe": "7.5.0",
    "tslib": "^1.9.0",
    "zone.js": "~0.9.1"
  },
  "devDependencies": {
    "@angular-builders/jest": "8.2.0",
    "@angular-devkit/build-angular": "~0.801.2",
    "@angular/cli": "~8.1.2",
    "@angular/compiler-cli": "~8.1.2",
    "@angular/language-service": "~8.1.2",
    "@types/cookie-parser": "1.4.1",
    "@types/cors": "2.8.5",
    "@types/express": "4.17.0",
    "@types/gsap": "1.20.2",
    "@types/jest": "24.0.18",
    "@types/node": "~8.9.4",
    "@types/stripe": "6.31.6",
    "codelyzer": "^5.0.1",
    "commander": "3.0.1",
    "jest": "24.9.0",
    "protractor": "~5.4.0",
    "shelljs": "0.8.3",
    "strip-comments": "1.0.2",
    "ts-loader": "6.0.4",
    "ts-node": "~7.0.0",
    "tsc-watch": "2.4.0",
    "tsconfig-paths-webpack-plugin": "3.2.0",
    "tscpaths": "0.0.9",
    "tslint": "~5.11.0",
    "typescript": "~3.4.5",
    "webpack-cli": "3.3.6"
  }
}
