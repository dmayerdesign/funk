{
  "name": "funk",
  "version": "0.0.0",
  "scripts": {
    "build": "npm run ui::build && npm run functions::build",
    "deploy": "firebase deploy",
    "serve": "npm run ui::serve",
    "start": "npm run serve",
    "ui::build": "ng build --prod --configuration=production; ng run web:server --configuration=production",
    "ui::deploy": "npm run ui::web::deploy",
    "ui::serve": "npm run ui::web::serve",
    "ui::web::build": "ng build --project=web",
    "ui::web::deploy": "firebase deploy --only hosting",
    "ui::web::serve": "ng serve --project=web",
    "ui::web::test": "ng test --project=web",
    "ui::web::lint": "ng lint --project=web",
    "ui::web::e2e": "ng e2e --project=web",
    "functions::build": "tsc --project functions/tsconfig.json && tscpaths -p ./functions/tsconfig.json -s . -o ./functions/lib; node functions::postbuild",
    "functions::deploy": "firebase deploy --only functions",
    "functions::serve": "npm run functions::build && firebase serve --only functions",
    "functions::start": "npm run functions::build && firebase functions:shell",
    "functions::logs": "firebase functions:log",
    "ng": "ng"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "8.1.3",
    "@angular/cdk": "8.1.2",
    "@angular/common": "~8.1.2",
    "@angular/compiler": "~8.1.2",
    "@angular/core": "~8.1.2",
    "@angular/flex-layout": "8.0.0-beta.26",
    "@angular/forms": "~8.1.2",
    "@angular/material": "8.1.2",
    "@angular/platform-browser": "~8.1.2",
    "@angular/platform-browser-dynamic": "~8.1.2",
    "@angular/platform-server": "~8.1.2",
    "@angular/pwa": "0.801.3",
    "@angular/router": "~8.1.2",
    "@angular/service-worker": "~8.1.2",
    "@google-cloud/firestore": "2.2.4",
    "core-js": "^2.5.4",
    "gsap": "2.1.3",
    "hammerjs": "^2.0.8",
    "ng-click-outside": "4.0.0",
    "rxjs": "~6.5.2",
    "stripe": "7.5.0",
    "tslib": "^1.9.0",
    "zone.js": "~0.9.1"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.801.2",
    "@angular/cli": "~8.1.2",
    "@angular/compiler-cli": "~8.1.2",
    "@angular/language-service": "~8.1.2",
    "@types/gsap": "1.20.2",
    "@types/jasmine": "~2.8.8",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "~8.9.4",
    "@types/stripe": "6.31.6",
    "codelyzer": "^5.0.1",
    "jasmine-core": "~2.99.1",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~3.0.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.1",
    "karma-jasmine": "~1.1.2",
    "karma-jasmine-html-reporter": "^0.2.2",
    "protractor": "~5.4.0",
    "ts-loader": "6.0.4",
    "ts-node": "~7.0.0",
    "tsconfig-paths-webpack-plugin": "3.2.0",
    "tscpaths": "0.0.9",
    "tslint": "~5.11.0",
    "typescript": "~3.4.5",
    "webpack-cli": "3.3.6"
  }
}
