{
  "extends": "../tsconfig.json",
  "compilerOptions": {
    "outDir": "./out-tsc/app",
    "baseUrl": ".",
    "paths": {
      "@funk/api/*": [ "../api/interface/*" ],
      "@funk/config": [ "../.funk/build-pipeline-output/config/config.ts" ],
      "@funk/copy/*": [ "../copy/*" ],
      "@funk/helpers/*": [ "../helpers/*" ],
      "@funk/model/*": [ "../model/*" ],
      "@funk/plugins/persistence/*": [ "../plugins/implementation/persistence/web/*" ],
      "@funk/plugins/*": [ "../plugins/implementation/*" ],
      "@funk/plugins-spec/*": [ "../plugins/spec/*" ],
      "@funk/ui/*": [ "./src/*" ]
    }
  },
  "files": [
    "src/main.ts",
    "src/polyfills.ts"
  ],
  "include": [
    "src/**/*.d.ts"
  ]
}
